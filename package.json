{
  "name": "hello-week",
  "version": "2.10.2",
  "description": "Lightweight and simple calendar with no dependencies in pure JavaScript",
  "license": "MIT",
  "repository": "https://github.com/maurovieirareis/hello-week",
  "keywords": [
    "yarn",
    "webpack",
    "typescript",
    "javascript",
    "scss",
    "datepicker",
    "calendar"
  ],
  "author": "Mauro Reis Vieira <mauroreisvieira@gmail.com>",
  "bugs": {
    "url": "https://github.com/maurovieirareis/hello-week/issues"
  },
  "ignore": [
    "node_modules"
  ],
  "scripts": {
    "watch": "rollup -c -w",
    "build": "NODE_ENV=production rimraf dist && npm run sass:build && rollup -c --environment NODE_ENV:production",
    "sass:build": "node-sass src/css/hello-week-theme.scss dist/hello-week.theme.min.css && node-sass src/css/hello-week.scss dist/hello-week.min.css --output-style compressed",
    "serve": "http-server -c0 -g",
    "test": "karma start"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "karma": "^4.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-typescript": "^4.0.0",
    "mocha": "^6.0.0",
    "node-sass": "^4.12.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.22.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  }
}
